					{
						"targets": {
							"node": ["7.6.0"]
						}
					}
				]
			],
			"plugins": [
				"transform-object-rest-spread",
				"add-module-exports"
			]
		},
		"old": {
			"presets": [
				[
					"env",
					{
						"targets": {
